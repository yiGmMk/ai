        <pubDate>Tue, 05 Aug 2025 00:00:00 +0000</pubDate>
        
        <guid>https://ai.programnotes.cn/p/xai-%E5%8F%91%E5%B8%83-grok-imagine-4%E6%94%AF%E6%8C%81%E6%96%87%E7%94%9F%E5%9B%BE/%E8%A7%86%E9%A2%91%E5%8F%8A-nsfw-%E5%86%85%E5%AE%B9%E7%94%9F%E6%88%90/</guid>
        <description>&lt;p&gt;&lt;a class=&#34;link&#34; href=&#34;https://x.com/elonmusk/status/1952052667502731723&#34;  target=&#34;_blank&#34; rel=&#34;noopener&#34;
